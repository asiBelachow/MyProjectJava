package model;

public interface Model {
	

	/**
	 * <h1>Get directory path</h1><p>
	 * <i><ul>void getDirPath(String path)<i><p>
	 * Display all files and folders in a given directory
	 * @param String- the directory
	 */
	public void dirPath(String path);
	
	
	/**
	 * <h1>Generate Maze3D</h1><p>
	 * <i><ul>void generateMaze3D(String mazeName)<i><p>
	 * Generate new Maze3D
	 * @param String- name of Maze3D
	 */
	public void generateMaze3D(String mazeName);
	
	
	/**
	 * <h1>Cross Section By Dimension</h1><p>
	 * <i><ul>void CrossSectionByDimention(String command, int dimension, String mazeName)<i><p>
	 * Generate a cross section of Maze3D by given dimension .
	 * @param crossSection
	 * @param dimension 
	 * @param mazeName 
	 */
	public void CrossSectionByDimention(String crossSection, int dimension, String mazeName);
	
	
	
	
	/**
	 * <h1>Save Maze</h1><p>
	 * <i><ul>void saveMaze(String mazeName, String file)<i><p>
	 * @param mazeName the maze name
	 * @param file the file
	 */
	public void saveMaze(String mazeName, String file);
	
	
	
		


}
