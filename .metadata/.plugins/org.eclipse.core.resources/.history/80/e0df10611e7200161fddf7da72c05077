package boot;

import controller.MyController;
import model.MyModel;
import view.MyView;

public class Run {

	public static void main(String[] args) {
		
		MyController c = new MyController();
		MyModel m = new MyModel(c);
		MyView v = new MyView(c);
		c.setModel(m);
		c.setView(v);
		c.setCommands();
		
		

	}

}
