package view;

import controller.Controller;

public class MyView extends MyCommonView {
	
	
	//------------------------------Constructors-------------------------//
	
	public MyView(Controller cont) {
		super(cont);
	}

	
	
	
	//-----------------------------setters and getters-------------------//
	
	
	
	
	
	//-------------------------Functionality-------------------------//
	
	
	@Override
	public void getCommand(String command, String[] parm) {
		
		cont.forwardCommand(command, param);
		
	}
}
