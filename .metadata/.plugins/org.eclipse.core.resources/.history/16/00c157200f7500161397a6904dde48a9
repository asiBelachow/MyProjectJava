package allCommands;

import java.io.IOException;

import model.Model;

/**
 * <h1>Save Maze</h1><p>
 * This Class define the command to load a Maze3D</br>
 * that saved to file by the name of the file
 */
public class SaveMaze extends ClientCommands {

	public SaveMaze(Model m) {
		super(m);

	}

	@Override
	public void doCommmand(String[] param) throws IOException {

		m.saveMaze(param[2], param[3]);
	}
	
	@Override
	public String toString() {
		return getClass().getName();
	}

	

}
