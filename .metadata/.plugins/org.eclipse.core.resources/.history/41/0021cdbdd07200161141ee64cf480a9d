package view;

import java.io.IOException;



/**
 * This interface is the view part of our MVC architecture, used for displaying
 * server status/client command inputs and controlling the server and its
 * clients
 * 
 * @author Asi Belachow
 * @version 1.0
 * @since 2016-23-08
 *
 */
public interface View {
	
	void start();
	
	public void forwardCommand(String command, String[] parm) throws IOException;
	
	public void displaySolution(String solution);

}
